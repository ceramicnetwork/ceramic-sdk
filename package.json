{
  "name": "ceramic-sdk",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "build": "pnpm --filter \"@ceramic-sdk/*\" build:types && turbo run build:js",
    "lint": "biome check --write apps/* packages/* tests/*",
    "lint:ci": "biome ci packages/* tests/*",
    "test": "turbo run test -- --passWithNoTests",
    "test:ci": "turbo run test:ci -- --passWithNoTests"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.6",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "del-cli": "^5.1.0",
    "jest": "^29.7.0",
    "tsx": "^4.17.0",
    "turbo": "^2.0.12",
    "typescript": "^5.5.2"
  },
  "pnpm": {
    "overrides": {}
  }
}
