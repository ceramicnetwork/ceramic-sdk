{
  "name": "ceramic-sdk",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "build": "pnpm --filter \"@ceramic-sdk/*\" build:types && turbo run build:js",
    "lint": "biome check --write packages/*",
    "lint:ci": "biome ci packages/*",
    "test": "turbo run test -- --passWithNoTests",
    "test:ci": "turbo run test:ci -- --passWithNoTests"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.1",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.5",
    "del-cli": "^5.1.0",
    "jest": "^29.7.0",
    "tsx": "^4.15.6",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {}
  }
}
